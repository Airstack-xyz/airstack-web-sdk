{
  "name": "@airstack/airstack-react",
  "version": "0.6.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build-static": "tsc && vite build --config vite.static.config.ts",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "ts-check": "tsc --noEmit",
    "preview": "vite preview",
    "test": "vitest",
    "copy-package-json": "cp package.json dist/ && cp README.md dist/",
    "publish-ready": "npm run build && npm run copy-package-json",
    "publish-package": "npm run publish-ready && cd dist/ && npm publish"
  },
  "dependencies": {
    "graphql": "16.6.0"
  },
  "peerDependencies": {
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0",
    "react-dom": "^16.8.0 || ^17.0.0 || ^18.0.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/jest": "^29.5.1",
    "@types/node": "^20.0.0",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "@vitejs/plugin-react": "^4.0.0",
    "cross-fetch": "^4.0.0",
    "eslint": "^8.38.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "jest": "^29.5.0",
    "jsdom": "^22.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup-plugin-rename-node-modules": "^1.3.1",
    "typescript": "^5.0.2",
    "vite": "^4.2.0",
    "vite-dts": "^1.0.4",
    "vite-plugin-dts": "^2.2.0",
    "vitest": "^0.31.0"
  },
  "files": [
    "*"
  ],
  "main": "./index.js",
  "module": "./index.js",
  "types": "./index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/Airstack-xyz/airstack-web-sdk"
  }
}
